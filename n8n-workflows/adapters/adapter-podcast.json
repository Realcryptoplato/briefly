    {
      "parameters": {
        "method": "POST",
        "url": "https://api.taddy.org",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Content-Type",
              "value": "application/json"
            },
            {
              "name": "X-USER-ID",
              "value": "={{ $env.TADDY_USER_ID }}"
            }
          ]
        },
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\n  \"query\": \"query GetPodcastEpisodes($uuid: ID!, $limitPerPage: Int, $page: Int) { getPodcastSeries(uuid: $uuid) { uuid name description episodes(limitPerPage: $limitPerPage, page: $page) { uuid name description datePublished audioUrl duration contentUrl } } }\",\n  \"variables\": {\n    \"uuid\": \"{{ $json.platform_id || $json.identifier }}\",\n    \"limitPerPage\": 10,\n    \"page\": 1\n  }\n}",
        "options": {
          "response": {
            "response": {
              "responseFormat": "json"
            }
          }
        }
      },
      "id": "http-taddy-1",
      "name": "Query Taddy API",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [
        1740,
        400
      ],
      "credentials": {
        "httpHeaderAuth": {
          "id": "taddy-api-key",
          "name": "Taddy API Key"
        }
      },
      "onError": "continueErrorOutput"
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "has-podcast",
              "leftValue": "={{ $json.data?.getPodcastSeries }}",
              "rightValue": "",
              "operator": {
                "type": "object",
                "operation": "isNotEmpty"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "if-podcast-found-1",
      "name": "Podcast Found?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [
        1960,
        400
      ]
    },
    {
      "parameters": {
        "jsCode": "// Filter and transform podcast episodes\nconst response = $input.first().json;\nconst podcast = response.data?.getPodcastSeries;\n\nif (!podcast) {\n  return [{\n    json: {\n      items: [],\n      count: 0,\n      podcast_id: null,\n      podcast_name: null\n    }\n  }];\n}\n\nconst episodes = podcast.episodes || [];\nconst hoursBack = $('Initialize').item.json.hours_back;\nconst cutoffDate = new Date();\ncutoffDate.setHours(cutoffDate.getHours() - hoursBack);\n\nconst recentEpisodes = episodes.filter(ep => {\n  if (!ep.datePublished) return false;\n  const pubDate = new Date(ep.datePublished);\n  return pubDate >= cutoffDate;\n});\n\nconst items = recentEpisodes.map(ep => ({\n  platform: 'podcast',\n  platform_id: ep.uuid,\n  source_identifier: podcast.uuid,\n  source_name: podcast.name,\n  content: ep.description || '',\n  url: ep.contentUrl || `https://taddy.org/podcast/${podcast.uuid}/episode/${ep.uuid}`,\n  posted_at: ep.datePublished,\n  fetched_at: new Date().toISOString(),\n  metrics: {\n    duration_seconds: ep.duration || 0\n  },\n  metadata: {\n    title: ep.name,\n    audio_url: ep.audioUrl,\n    has_transcript: false,\n    transcript_source: null,\n    can_transcribe_locally: !!ep.audioUrl,\n    podcast_description: podcast.description\n  }\n}));\n\nreturn [{\n  json: {\n    items,\n    count: items.length,\n    podcast_id: podcast.uuid,\n    podcast_name: podcast.name\n  }\n}];"
      },
      "id": "code-transform-1",
      "name": "Transform Episodes",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        2200,
        300
      ]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "needs-transcription",
              "leftValue": "={{ $json.items.some(i => i.metadata.can_transcribe_locally && !i.metadata.has_transcript) }}",
              "rightValue": true,
              "operator": {
                "type": "boolean",
                "operation": "equals"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "if-needs-transcription-1",
      "name": "Needs Transcription?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [
        2420,
        300
      ]
    },
    {
      "parameters": {
        "jsCode": "// Queue episodes for transcription\nconst items = $input.first().json.items;\nconst needsTranscription = items.filter(i => \n  i.metadata.can_transcribe_locally && !i.metadata.has_transcript\n);\n\nconst transcriptionQueue = needsTranscription.map(item => ({\n  platform: 'podcast',\n  platform_id: item.platform_id,\n  audio_url: item.metadata.audio_url,\n  duration_seconds: item.metrics.duration_seconds,\n  source_name: item.source_name,\n  title: item.metadata.title\n}));\n\nreturn [{\n  json: {\n    items: $input.first().json.items,\n    count: $input.first().json.count,\n    podcast_id: $input.first().json.podcast_id,\n    podcast_name: $input.first().json.podcast_name,\n    transcription_queue: transcriptionQueue,\n    transcription_count: transcriptionQueue.length\n  }\n}];"
      },
      "id": "code-queue-transcription-1",
      "name": "Queue Transcription",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        2660,
        200
      ]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "={{ $env.BRIEFLY_API_URL }}/api/content/transcribe",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={{ JSON.stringify({ items: $json.transcription_queue }) }}",
        "options": {
          "response": {
